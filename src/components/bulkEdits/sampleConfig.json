{
    "google": {
      "config": {
        "type": "oidc",
        "id": "google",
        "name": "Google",
        "config": {
          "issuer": "https://accounts.google.com",
          "clientID": "$GOOGLE_CLIENT_ID",
          "clientSecret": "$GOOGLE_CLIENT_SECRET",
          "redirectURI": "http://devtron.yourdomain.com/orchestrator/api/dex/callback",
          "hostedDomains": [
            "google.com"
          ]
        }
      }
    }
}